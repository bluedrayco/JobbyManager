{
  "name": "tecno-system/jobby-manager",
  "homepage": "https://github.com/bluedrayco/JobbyManager",
  "license": "MIT",
  "description": "Manage all your cron jobs without modifying crontab. Handles locking, logging, error emails, and more.",
  "authors": [
    {
      "name": "Roberto Leroy Monroy Ruiz",
      "email": "bluedrayco@gmail.com",
      "homepage": "https://github.com/bluedrayco"
    }
  ],
  "require": {
    "php": ">=7.0",
    "dragonmantank/cron-expression": "^2.0",
    "maknz/slack": "^1.7",
    "opis/closure": "^3.5",
    "swiftmailer/swiftmailer": "^6.0",
    "symfony/process": "^3.0"
  },
  "require-dev": {
    "mp091689/dump-die": "^1.0",
    "phpunit/phpunit": "^6.0",
    "symfony/filesystem": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Jobby\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jobby\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}
